:@template"ûM@import "compass/css3";


/*---------- Difference client boxes ----------*/

.difference-rotate-block {
    .difference-rotate {
        width: 100%;

        .difference-candiate {
            width: 555px;

            .header-block {
                color: rgba( 255, 255, 255, 0.4 );
                text-transform: uppercase;
                font-size: 18px;
                font-weight: bold;
                text-align: right;
                padding: 0 5px;
            }

            .canidate-content {
                @include background-image(linear-gradient(#343333, #141415));

                .canidate-pads {
                    padding: 10px 20px;

                    h2 {
                        font-size: 30px;
                        text-transform: uppercase;
                        font-weight: bold;
                        color: #d3d3d3;
                        margin:0 0 12px 0;
                    }

                    .logo-description-block {
                        a {
                            float: left;

                            &:hover {
                                img {
                                    @include opacity( 0.6 );
                                }
                            }
                        }

                        .canidate-name-block {
                            float: left;
                            color: #D3D3D3;
                            padding-left: 22px;

                            h3 {
                                font-size: 20px;
                                font-weight: bold;
                                line-height: 26px;
                                overflow: auto;

                                span {
                                    font-weight: normal;
                                    display: block;
                                }

                                a {
                                    color: #D3D3D3;
                                    text-decoration: none;
                                }
                            }

                            .timer-view-block {
                                margin: 5px 0 0 0;

                                li {
                                    font-size: 18px;
                                    text-transform: uppercase;
                                    float: left;
                                    border-right: 1px solid #D3D3D3;
                                    padding: 0 15px;

                                    &.title-case {
                                        text-transform: none;
                                    }

                                    &:first-child {
                                        padding-left: 0;
                                    }

                                    &:last-child {
                                        border-right: none;
                                    }

                                    a {
                                        font-size: 18px;
                                        color: #bebdbd;
                                        text-transform: lowercase;
                                        text-decoration: none;
                                        position: relative;
                                        
                                    }
                                }
                            }
                        }
                    }

                    .date-goal-block {
                        clear: both;
                        color: #FFFFFF;
                        margin-top: 10px;

                        .date-goal-txt {
                            text-transform: uppercase;
                            font-size: 18px;

                            span {
                                font-size: 20px;
                                font-weight: bold;
                                letter-spacing: 0.5px;
                            }

                            .to-date {
                                float: left;
                                margin: 0 !important;
                                span {
                                    color: #FF0000;
                                    font-family: 'Open Sans', sans-serif;
                                }
                            }

                            .main-goal {
                                float: right;
                                margin: 0 !important;
                            }
                        }
                    }

                    .progressbar-block {
                        .progressbar {
                            height: 22px;
                            border: none;
                            margin-top: 5px;
                            @include border-radius( 0 );

                            .ui-progressbar-value {
                                background: #e70000;
                                @include border-radius(0);

                                .progress-label {
                                    color: #FFFFFF;
                                    float: right;
                                    font-size: 13px;
                                    font-weight: 100;
                                    padding: 0 5px 0 0;

                                    &:first-child {
                                        display: none;
                                    }
                                }
                            }
                        }
                    }

                    .performance-block {
                        color: #d3d3d3;
                        position: relative;
                        margin-top: 5px;

                        .tooltip {
                            position: absolute;
                            background: #FFFFFF;
                            @include border-radius( 10px );
                            padding: 3px 25px 5px;
                            color: #000000;
                            font-size: 18px;
                            text-transform: uppercase;
                            font-weight: bold;
                            @include opacity( 0 );
                        }

                        ul {

                            margin:2px 0;

                            li {
                                text-transform: uppercase;
                                font-size: 18px;
                                float: left;
                                padding: 0 25px;

                                &.title-case {
                                    text-transform: none;
                                }

                                &:first-child {
                                    padding-left: 0;
                                    border-right: 1px solid #d3d3d3;
                                }

                                &:last-child {
                                    padding-right: 0;
                                    float:right;
                                }

                                .list-item-arow {
                                    font-weight: normal;
                                    padding: 0 5px;
                                    position: relative;
                                    top: -2px;
                                }

                                .red-txt {
                                    color: #FF0000;

                                }
                            }
                        }
                    }
                }
            }
        }

        .green {
            float: left;

            .header-block {
                background: #6db447;
            }

            .timer-view-block {
                li {
                    span {
                        color: #6db447;
                    }
                }
            }
        }

        .blue {
            float: right;

            .header-block {
                background: #6eafde;
            }

            .timer-view-block {
                li {
                    span {
                        color: #6eafde;
                    }
                }
            }
        }
    }
}

.button-container-campaign {
    text-align: center;
    margin-top: 25px;
    
    .red-btn{
        font-size:18px;
    }

}

#slide-nav-campaigns {
    width: 100%;

    li {
        position: absolute;
        top: 205px;

        &:first-child {
            left: -25px;
        }

        &:last-child {
            right: -25px;
        }
    }
}

/*---------- Short Slideshow ----------*/

.short-block-slideshow-full {
    height: 428px;
    padding-top: 110px;
    position: relative;
    min-width: 1170px;

    .slideshow-short {
        .short-slideshow-img {
            position: relative;
            height: 428px;
            width: 100%;
            background: url( '../images/sports-slide-01.jpg' ) center top / cover no-repeat;

            .short-slideshow-bar-full {
                position: absolute;
                bottom: 0;
                width: 100%;
                background: rgba( 0, 0, 0, 0.3 );

                .short-slideshow-centered {
                    .site-pads {
                        padding: 25px 15px 20px;

                        .bar-left {
                            position: relative;
                            float: left;
                            color: #FFFFFF;

                            img {
                                position: absolute;
                                width: 90px !important;
                            }

                            .short-slide-bar-txt {
                                padding: 8px 0 0 110px;

                                h2 {
                                    text-transform: uppercase;
                                    font-size: 35px;
                                    line-height: 39px;
                                    font-weight: bold;
                                    margin-bottom: 0;
                                    letter-spacing: 0.5px;
                                }

                                p {
                                    font-size: 20px;
                                }
                            }
                        }

                        .bar-right {
                            position: relative;
                            float: right;

                            .red-btn {
                                font-size: 17px;
                                margin-top: 25px;
                            }
                        }
                    }
                }
            }
        }
    }

    #slide-nav {
        bottom: 114px;
        position: absolute;
        z-index: 200;
        width: 100%;
        opacity: 0;

        li {
            a {
                position: absolute;
                top: 53px;
            }

            #prev {
                left: 6%;
            }

            #next {
                right: 6%;
            }
        }
    }

    .active {
        opacity: 1.0 !important;
    }
}

/*---------- Dropdown section ----------*/

#campaign-form {
    position: relative;
    ;
    float: left;
}

.form-dd {
    position: absolute;
    top: 20px;
    width: 329px; // Change width dependign on form section
    li {
        color: #FFFFFF;
        padding: 7px 15px;
        font-size: 16px;
        display: block;
        cursor: pointer;
        background: #a7a29a;

        &:hover {
            background: #bcb7b0;
        }
    }
}

/*---------- Pick sport/scholastics/services section ----------*/

.pick-block-full {
    padding-bottom: 50px;
    min-width: 1170px;

    .pick-block-centered {
        .site-pads {
            h2 {
                font-size: 28px;
                text-transform: uppercase;
                color: #FFFFFF;
                font-weight: bold;
                letter-spacing: 0.5px;
                text-align: center;
                padding: 25px 0 20px;
            }

            .pick-block-options {
                ul {
                    width: 1006px;
                    margin: 0 auto 15px;

                    &:last-child {
                        margin-bottom: 0;
                    }

                    li {
                        float: left;
                        margin-right: 15px;

                        &:last-child {
                            margin-right: 0;
                        }

                        a {

                            img {
                                width: 65px !important;
                            }

                            display: block;
                            width: 107px;
                            padding: 8px 0;
                            text-align: center;
                            color: #221f1f;
                            text-transform: uppercase;
                            text-decoration: none;
                            font-size: 15px;
                            letter-spacing: 0.3px;
                            @include border-radius( 8px );
                            @include box-shadow( 0 2px 2px rgba( 0, 0, 0, 0 ) );

                            .icon-hover {
                                position: absolute;
                                @include opacity( 0 );
                            }

                            .icon {
                                position: relative;
                                @include opacity( 1 );
                            }

                            span {
                                display: block;
                                padding-top: 10px;
                                line-height: 19px;
                            }

                            &:hover {
                                background: #FFFFFF;
                                @include box-shadow( 0 2px 2px rgba( 0, 0, 0, 0.4 ) );

                                .icon-hover {
                                    @include opacity( 1 );
                                }

                                .icon {
                                    @include opacity( 0 );
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}

/*---------- Notification bar ----------*/

.notification-full {
    padding-top: 145px;
    background: #9f9d9a;
    height: 64px;
    min-width: 1170px;

    .notification-centered {
        .site-pads {
            .notification-left {
                float: left;

                .avatar-block {
                    float: left;

                    img {
                        float: left;
                        @include border-radius( 50px );
                        border: 4px solid #d3d3d3;
                    }

                    span {
                        display: inline-block;
                        font-size: 22px;
                        color: #373838;
                        text-transform: uppercase;
                        font-weight: bold;
                        padding: 6px 0 0 10px;
                    }
                }

                .notification-alert {
                    display: inline-block;
                    background: url( '../images/build-campaign/notification-alert-bg.png' ) 0 0 no-repeat;
                    width: 35px;
                    height: 33px;
                    margin: 4px 0 0 15px;

                    span {
                        padding: 15px;
                        color: #FFFFFF;
                        font-size: 22px;
                        font-weight: bold;
                    }
                }
            }

            .notification-right {
                float: right;

                ul {
                    margin: 7px 0 0 0;

                    li {
                        float: left;
                        border-right: 1px solid #373838;

                        &:last-child {
                            border-right: none;

                            a {
                                padding-right: 0;
                            }
                        }

                        a {
                            text-decoration: none;
                            font-size: 22px;
                            color: #373838;
                            font-weight: bold;
                            padding: 20px 15px 26px 15px;

                            &:hover {
                                color: #565656;
                            }
                        }

                        .active {
                            color: #9F9D9A;
                            background: #343333;

                            &:hover {
                                color: #9F9D9A;
                            }
                        }
                    }
                }
            }
        }
    }
}

/*---------- Spledge now popup ----------*/

.spledge-now-popup {
    width: 600px;
    background: #FFFFFF;
    display: none;
    @include border-radius( 5px );

    .title-block {
        position: relative;
        background: #2E302F;
        @include border-radius( 3px 3px 0 0 );
        text-align: center;
        color: #FFFFFF;
        padding: 15px 0 25px;

        .close {
            color: #ff0000;
            font-size: 20px;
            position: absolute;
            right: 20px;
            cursor: pointer;
        }

        h2 {
            font-size: 40px;
            font-weight: bold;
            letter-spacing: 0.5px;
        }

        p {
            font-size: 24px;
        }
    }

    .options-block {
        ul {
            margin: 0;

            li {
                display: block;
                color: #2E302F;
                overflow: auto;
                padding: 25px 20px;
                border-bottom: 1px solid #dadada;

                &:last-child {
                    border-bottom: none;
                }

                img {
                    width: 100px;
                    height: auto;
                    float: left;
                }

                .option-content {
                    margin-left: 20px;
                    float: left;

                    .red-btn {
                        float: right;
                        margin-top: 36px;
                    }

                    h3 {
                        font-size: 35px;
                        line-height: 24px;
                        font-weight: bold;
                        clear: none;
                        margin: 12px 0 10px;
                    }

                    p {
                        font-size: 24px;
                        width: 75%;
                        line-height: 26px;
                    }
                }
            }
        }
    }
}

.active-popup {
    display: block;
    position: absolute;
    z-index: 60001;
    left: 34%;
    top: 10%;
}

/*---------- Page not found ----------*/

.page-not-found-alert {
    display: block;
    margin: 50px auto;
    background: #f7f7f7;
    @include border-radius( 5px );
    border: 3px solid #FFFFFF;
    width: 708px;

    .page-not-found-pads {
        padding: 20px 35px;

        .page-not-found-img {
            float: left;
            margin-top: 22px;
        }

        .alert-txt {
            float: left;
            margin-left: 23px;
            font-size: 20px;
            line-height: 27px;
            width: 538px;

            .click-now {
                color: #6c6e6e;
                font-weight: bold;
                text-decoration: none;
                font-size: 18px;

                &:hover {
                    color: #000000;
                }
            }
        }
    }
}
