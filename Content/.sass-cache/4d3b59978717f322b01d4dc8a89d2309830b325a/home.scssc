:@template",@import "compass/css3";

.challenge-accepted-full {
	position: relative;;
	min-width: 1170px;
	height: 100%;
	.slideshow {
		height: 100% !important;
		width: 100%;
		position: relative;
		z-index: 0;
	}	
	.challenge-accepted-full-img {
		height: 100%;
		background: url( '../images/block-one.jpg' ) center top no-repeat !important;
		@include background-size( cover !important );
		// min-height: 777px !important;
		height: 100% !important;
		width: 100%;
		.challenge-accepted-centered {
			padding: 12% 0 0;
			.site-pads {
				.block-one-txt {
					text-align: center;
					color: #FFFFFF;
					letter-spacing: 0.5px;
					h1 {
						font-size: 50px;
						font-weight: bold;
						margin-bottom: 5px;
						@include text-shadow( 1px 2px 42px rgba(0, 0, 0, 0.9) );
						text-transform: uppercase;
					}
					p {
						font-size: 25px;
						line-height: 30px;
						@include text-shadow( 1px 0px 15px rgba(0, 0, 0, 1.0) );
					}
					ul {
						margin: 30px 0 35px;
						li {
							display: inline-block;
							margin-left: 47px;
							// &:first-child{
							// 	margin-left: 0;
							// }
							// &:last-child {
							// 	margin-left: 0;
							// }
						}
						.watch-video {
							a {
								display: none;
							}
						}
					}
					.vid-popup {
						width: 200px;
						height: auto;
					}
				}
			}
		}	
	}
	.black-bar-full {
		position: absolute;
		z-index: 149;
		bottom: 0;
		background: rgba( 0, 0, 0, 0.7 );
		width: 100%;
		padding: 20px 0;
		.black-bar-centered {
			.site-pads {
				.giant-form-block {
					width: 860px;
					margin: 0 auto;
				}
				h2 {
					color: #FFFFFF;
					font-size: 30px;
					font-weight: bold;
					text-transform: uppercase;
					float: left;
					margin-right: 15px;
				}
				#campaign-form {
					.campaign-title,
					.campaign-city {
						background: none;
						border-top: none;
						border-right: none;
						border-bottom: 1px solid #A7A29A;
						border-left: none;
						color: #A7A29A;
						float: left;
						padding: 9px 0;
					}
					.campaign-title {
						width: 328px;
						text-indent: 9px;
						border-right: 1px solid #A7A29A;
					}
					.campaign-city {
						background: url( '../images/map-icon.png' ) 10px center no-repeat;
						padding-left: 35px;
					}
					.submit-icon {
						color: #878788;
						font-size: 20px;
						padding-top: 4px;
					}
				}
			}
		}
	}
}
