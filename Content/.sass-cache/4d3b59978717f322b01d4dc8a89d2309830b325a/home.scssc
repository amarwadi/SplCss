.fundraising-facts-full {
	min-width: 1170px;
	.fundraising-facts-full-img {
		background: url( '../images/Home-Index/homepage-fundraising-section/Fundraising-never-more-fun.jpg' ) center top no-repeat !important;
		@include background-size( cover !important );
		height: 100% !important;
		padding: 60px 0;
		position: relative;
		&:before {
	    content: '';
			position: absolute;
			top: 0;
			right: 0;
			bottom: 0;
			left: 0;
			@include background-image(linear-gradient(left, rgba(255,255,255,1) 0%,  rgba(255,255,255,1) 20%, rgba(255,255,255,0) 22%, rgba(255,255,255,0) 50%, rgba(255,255,255,0) 78%, rgba(255,255,255,1) 82%, rgba(255,255,255,1) 100%));
		}
		.fundraising-facts-centered {
			.site-pads {
				h2 {
					color: #221f1f;
					font-size: 40px;
					line-height: 40px;
					font-weight: bold;
					text-transform: uppercase;
					margin-bottom: 25px;
					span {
						display: block;
					}
				}
				p {
					font-size: 25px;
					line-height: 28px;
					span {
						display: block;
					}
				}
				.fundrasing-list-perks {
					margin: 22px 0 0 0;
					li {
						font-size: 22px;
						background: url( '../images/black-check-mark.png' ) left 4px no-repeat;
						padding: 4px 0 1px 41px;
						margin-bottom: 5px;
						&:last-child {
							margin-bottom: 0;
						}
					}
				}
				.btn-container {
					margin: 35px 0 0 42px;
					li {
						float: left;
						margin-right: 25px;
						&:last-child {
							margin-right: 0;
						}
						.red-btn {
							padding: 9px 20px 9px;
						}
						.black-apple-btn {
							&:hover {
								img {
									@include opacity( 0.8 );
								}
							}
						}
					}
				}
			}
		}
	}
}
.testimonial-block-full {
	background: #f3f3f3;
	padding: 34px 0;
	min-width: 1170px;
	.testimonial-block-centered {
		.site-pads {
			h2 {
				font-size: 40px;
				color: #221f1e;
				font-weight: bold;
				text-transform: uppercase;
				text-align: center;
				margin-bottom: 25px;
			}
			.testimonial-row {
				.testimonial-block {
					float: left;
					width: 369px;
					background: url( '../images/small-shadow-right.jpg' ) center right no-repeat;
					min-height: 189px;
					margin-right: 16px;
					&:last-child {
						background: none;
						margin-right: 0;
					}
					.testimonial-pads {
						.testimonial-avatar {
							float: left;
						}
						.quote-block {
							float: left;
							width: 250px;
							margin-left: 20px;
							q {
								font-size: 19px;
								font-style: italic;
								letter-spacing: 0.5px;
							}
							cite {
								display: block;
								font-size: 18px;
								font-weight: bold;
								margin-top: 24px;
								span {
									display: block;
									font-weight: normal;
								}
							}
						}
					}
				}
			}
		}
	}
