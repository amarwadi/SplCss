:@template"..why-spledger-banner {
    img {
        width: 1170px;
    }
}

.why-spledger-full {
    background-color: #f3f3f3;
    min-width: 1170px;

    .why-spledger-centered {
        .site-pads {

            h2 {
                margin: 20px 0 10px;
                color: #282728;
                font-size: 30px;
                text-transform: uppercase;
                letter-spacing: 0.5px;
                font-weight: bold;
                text-align: center;
            }

            .why-spledger-banner {
                margin: 20px 0;
            }
        }
    }

    .why-spledger-table-sec {
        margin: 20px auto;
        width: 800px;
    }

    table {

        thead {
            tr {
                background-color: #6DB447;

                th {
                    padding: 10px 20px;
                    font-size: 35px;
                    font-weight: bold;
                    text-transform: uppercase;
                    color: #ffffff;

                    &.col1 {
                        width: 50%;
                    }

                    .right {
                        float: right;
                    }
                }
            }
        }

        tbody {
            tr {
                td {
                    padding: 10px 20px;

                    &.hi {
                        font-size: 45px;
                        font-weight: bold;
                        color: #6DB447;
                    }

                    &.lo {
                        text-align: left;
                        font-size: 25px;
                        font-weight: bold;

                        .hint {
                            font-size: 20px;
                        }
                    }
                }
            }
        }
    }

    ul {
        li {
            list-style-type: square;
            font-size: 22px;
            margin: 0 0 10px 30px;

            span {
                font-weight: bold;
            }
        }
    }

    .red-txt {
        font-size: 35px;
        font-weight: bold;
        text-align: center;
    }

    p {
        font-size: 20px;
        line-height: 26px;
        text-align: justify;
    }
}

.about-us-full {
    background-image: linear-gradient(#343333, #141415);
    min-width: 1170px;

    .about-us-centered {
        .site-pads {
            position: relative;
            text-align: center;
            color: #FFFFFF;

            h2 {
                font-size: 35px;
                line-height: 40px;
                font-weight: bold;
                text-transform: uppercase;
                padding: 20px 0 10px;

                span {
                    display: block;
                }
            }

            p {
                font-size: 21px;
                line-height: 27px;
                padding-bottom: 30px;
            }
        }
    }

    p {
        font-size: 20px;
        line-height: 26px;
    }
}

.about-us-content {
    p {
        font-size: 20px;
        line-height: 26px;
        margin: 20px 0 40px 0;

        span {
            &.emphasize {
                font-style: italic;
            }
        }
    }

    .circle-image-sec {
        margin: 20px auto;
        text-align: center;
    }
}

.faq-full {
    background-image: linear-gradient(#343333, #141415);
    min-width: 1170px;

    .faq-centered {
        .site-pads {
            position: relative;
            text-align: center;
            color: #FFFFFF;

            h2 {
                font-size: 30px;
                line-height: 40px;
                font-weight: bold;
                text-transform: uppercase;
                padding: 10px 0 10px;

                span {
                    display: block;
                }
            }

            p {
                font-size: 20px;
                line-height: 25px;
                padding-bottom: 30px;
            }
        }
    }
}

.faq-sec {
    overflow: hidden;

    p {
        font-size: 18px;
        line-height: 22px;
        margin: 10px 0 20px 0;
        text-align: justify;

        &.smallmarg {
            margin: 20px 0 10px 0;
        }
    }

    .faq-left {
        float: left;
        text-align: left;
        width: 25%;
        padding-bottom: 500em;
        margin-bottom: -500em;
        background-image: linear-gradient(#343333, #141415);

        .nav-tabs {
            li {
                background-color: transparent;
                padding: 5px 0;

                a {
                    color: #ffffff;
                    font-size: 18px;
                    text-decoration: none;
                    text-transform: uppercase;

                    &:hover {
                        text-decoration: none;
                        background-color: transparent;
                    }

                    &:focus {
                        background-color: transparent !important;
                    }
                }

                &.active {
                    background-color: #f8981d;

                    a {
                        color: #373838;
                    }
                }
            }
        }
    }

    .faq-right {
        float: right;
        width: 68%;
        padding-bottom: 500em;
        margin-bottom: -500em;
        text-align: left;
        padding-right: 45px;
    }

    .faq-content {

        h2 {
            font-weight: bold;
            font-size: 28px;
            text-transform: uppercase;
            margin: 25px 0;
        }

        .faq-item {
            h3 {
                font-weight: bold;
                font-size: 21px;
                margin-bottom: 0;
            }

            ol {
                margin-left: 30px;

                li {
                    font-size: 18px;
                    margin-bottom: 5px;
                    list-style-type: decimal;
                }
            }
        }
    }
}

.support-options-selections-full {
    margin-top: -10px;
}

.terms-full {
    background-color: #f3f3f3;
    min-width: 1170px;
    padding-top: 120px;

    h2 {
        font-weight: bold;
        font-size: 35px;
        text-transform: uppercase;
        margin: 30px 0;
    }

    h3 {
        font-weight: bold;
        font-size: 25px;
        margin-bottom: 20px;
    }

    p {
        font-size: 22px;
        line-height: 28px;
        margin: 20px 0 40px 0;
        text-align: justify;
    }

    .privacy-list {
        list-style-type: square;
        font-size: 22px;
        margin-left: 20px;
        margin-top: 0;

        li {
            margin-bottom: 10px;
            text-align: justify;
        }
    }
}

